# --- Database & General ---
DB_HOST=YOUR_DB_HOST                  # Example: localhost
DB_USER=YOUR_DB_USER                  # Example: postgres
DB_PASSWORD=YOUR_DB_PASSWORD            # Example: secret
DB_NAME=YOUR_DB_NAME                  # Example: scheduler_db
DB_PORT=YOUR_DB_PORT                  # Example: 5432

LOG_LEVEL=YOUR_LOG_LEVEL              # Options: debug, info, warn, error

# --- Compression Scheduler ---
COMPRESSION_SCHEDULE=YOUR_CRON_SCHEDULE     # Example: '*/5 * * * *'
COMPRESSION_SOURCE_DIR=YOUR_SOURCE_DIR         # Example: ./images/source
COMPRESSION_DEST_DIR=YOUR_DEST_DIR             # Example: ./images/compressed
COMPRESSION_IS_TEST_MODE=YOUR_IS_TEST_MODE      # Example: false
COMPRESSION_MAX_RETRIES=YOUR_MAX_RETRIES        # Example: 3
COMPRESSION_IS_CONCURRENT=YOUR_IS_CONCURRENT    # Example: true
COMPRESSION_BATCH_SIZE=YOUR_BATCH_SIZE        # Example: 50
COMPRESSION_NUM_IO_WORKERS=YOUR_NUM_IO_WORKERS  # Example: 8
COMPRESSION_NUM_CPU_WORKERS=YOUR_NUM_CPU_WORKERS# Example: 4
COMPRESSION_WEBP_QUALITY=YOUR_WEBP_QUALITY      # Example: 75
COMPRESSION_MAX_WIDTH=YOUR_MAX_WIDTH          # Example: 1920
COMPRESSION_MAX_HEIGHT=YOUR_MAX_HEIGHT        # Example: 1080

# --- Cleanup Scheduler (for old .webp files) ---
CLEANUP_SCHEDULE=YOUR_CRON_SCHEDULE         # Example: '0 2 * * *'
CLEANUP_THRESHOLD=YOUR_CLEANUP_THRESHOLD      # Example: 720h (30 days)
CLEANUP_BATCH_SIZE=YOUR_BATCH_SIZE          # Example: 100

# --- Janitor Scheduler (for stuck tasks) ---
JANITOR_SCHEDULE=YOUR_CRON_SCHEDULE         # Example: '*/15 * * * *'
JANITOR_STUCK_THRESHOLD=YOUR_JANITOR_THRESHOLD     # Example: 30m

# --- Deletion Queue Scheduler (for original source files) ---
DELETION_QUEUE_SCHEDULE=YOUR_CRON_SCHEDULE     # Example: '*/5 * * * *'
DELETION_QUEUE_BATCH_SIZE=YOUR_BATCH_SIZE        # Example: 100
DELETION_QUEUE_MAX_RETRIES=YOUR_MAX_RETRIES        # Example: 5